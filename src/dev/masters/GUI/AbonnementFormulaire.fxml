<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="598.0" prefWidth="803.0" style="-fx-background-color: #f29999;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.masters.GUI.AbonnementFormulaire">
   <children>
      <VBox layoutX="14.0" layoutY="61.0" prefHeight="297.0" prefWidth="164.0" spacing="45.0">
         <children>
            <Label text="Choisir un abonnement" />
            <Label text="Téléphone" />
            <Label text="Prix mensuel" />
            <Label text="Prix semestriel" />
            <Label text="Prix Annuel" />
         </children>
      </VBox>
      <VBox layoutX="178.0" layoutY="61.0" prefHeight="288.0" prefWidth="156.0" spacing="40.0">
         <children>
            <ComboBox fx:id="cbAbonn" onAction="#getSelectedItem" prefHeight="25.0" prefWidth="157.0" />
            <TextField fx:id="tfTel" />
            <RadioButton fx:id="rdMois" mnemonicParsing="false" />
            <RadioButton fx:id="rdSemestre" mnemonicParsing="false" />
            <RadioButton fx:id="rdAnnee" mnemonicParsing="false" />
         </children>
      </VBox>
      <Button layoutX="708.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#menu" style="-fx-background-color: #86C6FA;" text="Menu" />
      <Button fx:id="retourne" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#retourne" style="-fx-background-color: #FF336D;" text="Retourne" />
      <Button layoutX="53.0" layoutY="400.0" mnemonicParsing="false" onMouseClicked="#onSubmit" style="-fx-background-color: #1ab230;" text="Valider" />
   </children>
</AnchorPane>
